<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="AutomationIntegrationServiceR4_5Binding" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://cnshmes002.cn.kostal.int:50000/AutomationIntegrationServiceR4_5/AutomationIntegrationBean5" binding="basicHttpBinding" bindingConfiguration="AutomationIntegrationServiceR4_5Binding" contract="MES.AutomationIntegrationServiceR4_5" name="AutomationIntegrationPort5" />
        </client>
    </system.serviceModel>

  <appSettings>
    <!--扫描仪-->
    <add key="Scanner" value="\scanners\serial\x1900.dll" />
    <!--编码规则-->
    <add key="BarcodeRule" value="\barcoderules\standard.dll" />
    <!--执行序列-->
    <add key="Sequence" value="start-complete" />

    <add key="PanelReleaseSleep" value="5000"/>
  </appSettings>
</configuration>