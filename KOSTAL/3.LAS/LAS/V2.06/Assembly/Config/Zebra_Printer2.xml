<?xml version="1.0" encoding="utf-8"?>
<LasConfiguration xmlns="http://www.kostal.com/Testman/config">
  <!-- If Display NewPart Message -->
  <GeneralInformation>
    <!-- If configure Zebra by Program -->
    <ExternalInterface>False</ExternalInterface>
	<!-- Print Empty Label -->
    <EmptyLable>True</EmptyLable>
	<!-- IP Address -->
     <PrinterIP>169.254.157.117</PrinterIP>
	<!-- IP Port or COM Port, for example 9100 or COM1 -->
    <Port>9100</Port>
	<!-- COM Port Baud rate -->
    <Baud>9600</Baud>
	<!-- 
	    'ZEBRA_LANGUAGE_ENGLISH = 1
        'ZEBRA_LANGUAGE_SPANISH = 2
        'ZEBRA_LANGUAGE_FRENCH = 3
        'ZEBRA_LANGUAGE_GERMAN = 4
        'ZEBRA_LANGUAGE_ITALIAN = 5
        'ZEBRA_LANGUAGE_NORWEGIAN = 6
        'ZEBRA_LANGUAGE_PORTUGUESE = 7
        'ZEBRA_LANGUAGE_SWEDISH = 8
        'ZEBRA_LANGUAGE_DANISH = 9
        'ZEBRA_LANGUAGE_SPANISH2 = 10
        'ZEBRA_LANGUAGE_DUTCH = 11
        'ZEBRA_LANGUAGE_FINNISH = 12
        'ZEBRA_LANGUAGE_JAPANESE = 13
        'ZEBRA_LANGUAGE_KOREAN = 14
        'ZEBRA_LANGUAGE_CHINESE_SPECIFIED = 15
        'ZEBRA_LANGUAGE_CHINESE_TRADITIONAL = 16
	-->
